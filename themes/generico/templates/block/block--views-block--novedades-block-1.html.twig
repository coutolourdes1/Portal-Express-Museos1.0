<div{{ attributes.addClass(['component-novedades']) }}>
    {{ title_prefix }}
    {{ title_suffix }}
    {% if label %}
        <h3{{ attributes.addClass(['section__title']) }}>{{ label }}</h3>
    {% endif %}
    {% block content %}
        <div class="novedades__list">
            {{ content }}
        </div>
    {% endblock %}
    <div>
        <a href="#" class="btn-large btn-primary-outline btn-icon"> 
            ver todas  
            <span class="material-icons-outlined"> arrow_forward </span>
          </a>
    </div>
</div>
